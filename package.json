{
  "name": "udischeduler",
  "version": "3.2.25",
  "bin": {
    "next-forge": "dist/index.js"
  },
  "files": ["dist/index.js"],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "db:migrate": "cd packages/database && bunx drizzle-kit migrate",
    "db:generate": "cd packages/database && bunx drizzle-kit generate",
    "db:pull": "cd packages/database && bunx drizzle-kit pull",
    "lint": "ultracite lint",
    "format": "ultracite format",
    "test": "turbo test",
    "analyze": "turbo analyze",
    "bump-deps": "bunx npm-check-updates --deep -u -x react-day-picker,tailwindcss && bun install",
    "bump-ui": "bunx shadcn@latest add --all --overwrite -c packages/design-system",
    "migrate": "cd packages/database && bunx prisma format && bunx prisma generate && bunx prisma db push",
    "clean": "git clean -xdf node_modules"
  },
  "devDependencies": {
    "@auto-it/first-time-contributor": "^11.3.0",
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.28.1",
    "@turbo/gen": "^2.4.4",
    "@types/node": "^22.13.5",
    "@udi/typescript-config": "*",
    "tsup": "^8.4.0",
    "turbo": "^2.4.4",
    "typescript": "^5.7.3",
    "ultracite": "^4.1.20",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.43",
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "commander": "^13.1.0"
  },
  "type": "module",
  "workspaces": ["apps/*", "packages/*"],
  "trustedDependencies": [
    "@biomejs/biome",
    "@sentry/cli",
    "bufferutil",
    "core-js",
    "core-js-pure",
    "esbuild",
    "sharp"
  ]
}
